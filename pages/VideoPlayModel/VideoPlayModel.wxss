

.top {
  width: 100%;
  height: 230px;
  background: #23EBB9;
  padding-top: 15px;
  position: relative;
}


.center {
  width: 95%;
  height: 150px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 70rpx auto;
  border-radius: 5px;
}

.center_top {
  display: flex;
  flex-direction: row;
  width: 80%;
  height: 80px;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid #EEEEEE;
}

.center_top text{
  margin-top: 50rpx;
  margin-left: 45rpx;
  font-size: 44rpx;
  font-weight: 600;

}


.center_top image {
  width: 35px;
  height: 35px;
  margin-top: 50rpx;
  margin-left: 150rpx;
}



.gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}
/* jjjjj */
view{
  box-sizing: border-box;
}
.item-box{
  width: 700rpx;
  margin: 0 auto;
  padding:40rpx 0;
}
.items{
  width: 100%;
}
.item{
  position: relative;
  border-top: 2rpx solid #eee;
  height: 610rpx;
  line-height: 540rpx;
  overflow: hidden;
  text-align: center;
}

.item .videoPlayer{
  width: 100%;
  height: 420rpx;
  margin-top: -200rpx;
}

.inner.txt{
  background-color: #fff;
  width: 100%;
  padding:0 10rpx;
  margin-top: -210rpx;
  font-weight: 600;
  transition: left 0.2s ease-in-out;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
